{
    "name": "zk2/human-to-tsquery",
    "description": "Convert human request to postgres ts_query",
    "homepage": "https://github.com/zk2/human-to-tsquery",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Evgeniy Budanov",
            "email": "budanov.ua@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Zk2\\HumanToTsquery\\": "src/Zk2/HumanToTsquery"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Zk2\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8"
    },
    "require-dev": {
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*",
        "phpunit/phpunit": "^8.5",
        "doctrine/dbal": "^2.10",
        "symfony/var-dumper": "^5.0",
        "1happyplace/phpunit-colors": "^1.1",
        "elasticsearch/elasticsearch": "^8.8",
        "ext-curl": "*"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
